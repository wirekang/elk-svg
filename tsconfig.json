{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["DOM", "DOM.Iterable", "ES2017"],
    "module": "ES2015",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true
  }
}
